<html>
    <head>
        <title>todo application</title>
        <script src="https://code.jquery.com/jquery-1.12.3.min.js" integrity="sha256-aaODHAgvwQW1bFOGXMeX+pC4PZIPsvn2h1sArYOhgXQ="
         crossorigin="anonymous"></script>       
        <script src='/assets/todo-list.js'></script>
        <link href="/assets/styles.css" rel="stylesheet" type="text/css"/>
    </head>
    <body>
        <h1>Todo App</h1>
        <div id="todo-table">
            <form>
                <input type="text" name="item" placeholder="Add new item"/>
                <button type="submit">Add</button>
            </form>
            <ul>
                <% for(var i=0; i < todos.length; i++) { %>
                <li><%= todos[i].item %></li>
                <% } %>
            </ul>
        </div>
    </body>
</html>